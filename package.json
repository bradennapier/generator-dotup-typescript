{
  "name": "generator-dotup-typescript",
  "version": "0.0.1",
  "description": "Generator for typescript apps and libraries",
  "homepage": "https://github.com/dotupNET/generator-dotup-typescript",
  "author": {
    "name": "Peter Ullrich",
    "email": "peda76@gmail.com",
    "url": "https://github.com/dotupNET/"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "yeoman",
    "typescript",
    "generator",
    "test",
    "ci",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^1.9.1",
    "yeoman-assert": "^3.1.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.15.1",
    "prettier": "^1.16.4",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-config-xo": "^0.26.0",
    "jest": "^24.5.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "yeoman-generator": "^3.2.0",
    "chalk": "^2.4.2",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest",
    "postpublish": "git tag -a v%npm_package_version% -m \"Release v%npm_package_version%\" && git push origin --tags",
    "npm-publish": "npm publish"
  },
  "repository": "https://github.com/dotupNET/generator-dotup-typescript.git",
  "license": "MIT"
}