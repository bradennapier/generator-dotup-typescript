{
  "name": "generator-dotup-typescript",
  "version": "0.0.1",
  "description": "Generator for typescript apps and libraries",
  "homepage": "https://github.com/dotupNET/generator-dotup-typescript",
  "author": {
    "name": "Peter Ullrich",
    "email": "peda76@gmail.com",
    "url": "https://github.com/dotupNET/"
  },
  "scripts": {
    "build": "tsc",
    "release": "npm run clean-build && npm run test-mocha && git push && npm run gh-pages && npm publish",
    "clean-dist": "rimraf dist",
    "clean-build": "rimraf dist && tsc",
    "test-mocha": "node node_modules/mocha/bin/mocha --require ts-node/register test/*.test.ts --reporter spec",
    "test-jasmine": "jasmine-ts",
    "docs": "typedoc --mode file --out docs src --exclude \"**/*+(index|.spec|.test|.e2e).ts\"",
    "gh-pages": "rimraf docs && npm run docs && gh-pages -d docs",
    "postversion": "git push && git push --tags",
    "npm-publish": "npm publish"
  },
  "files": [
    "generators"
  ],
  "main": "generators/app/index.js",
  "keywords": [
    "yeoman",
    "typescript",
    "generator",
    "test",
    "ci",
    "yeoman-generator"
  ],
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/ini": "^1.3.30",
    "@types/js-base64": "^2.3.1",
    "@types/lodash": "^4.14.123",
    "@types/mocha": "^5.2.6",
    "@types/yeoman-generator": "^3.1.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "gh-pages": "^2.0.1",
    "grunt": "^1.0.3",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-run": "^0.8.1",
    "grunt-ts": "^6.0.0-beta.22",
    "grunt-tslint": "^5.0.2",
    "mocha": "^6.0.2",
    "rimraf": "^2.6.3",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-microsoft-contrib": "^6.0.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.3.3",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^1.9.1"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "@types/nodegit": "^0.24.4",
    "@types/request": "^2.48.1",
    "chalk": "^2.4.2",
    "dotup-ts-types": "0.0.19",
    "git-remote-origin-url": "^2.0.0",
    "github-api": "^3.0.0",
    "ini": "^1.3.5",
    "js-base64": "^2.5.1",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "nodegit": "^0.24.1",
    "npm-api": "^0.4.12",
    "typed-rest-client": "^1.1.2",
    "yeoman-generator": "^3.2.0",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "https://github.com/dotupNET/generator-dotup-typescript.git",
  "license": "MIT"
}
